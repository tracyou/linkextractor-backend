		<xsd:attribute name="tekstlengte" type="xsd:integer"/>
		<xsd:attribute name="metadatalengte" type="xsd:integer"/>
		<xsd:attribute name="metadatacontrole" type="xsd:string"/>
		<xsd:attribute name="controle"/>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="r2">
		<xsd:attribute name="bron"/>
		<xsd:attribute name="effect"/>
		<xsd:attribute name="inwerking" type="xsd:date"/>
		<xsd:attribute name="voorlopige-toepassing" type="xsd:date"/>
		<xsd:attribute name="ondertekening_bron" type="xsd:date"/>
		<xsd:attribute name="publicatie_bron" type="xsd:date"/>
		<xsd:attribute name="publicatie_iwt" type="xsd:date"/>
		<xsd:attribute name="publicatie_vt" type="xsd:date"/>
	</xsd:attributeGroup>

	<xsd:element name="jci">
		<xsd:complexType>
			<xsd:attributeGroup ref="attlist.jci"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="jcis">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="jci" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:attributeGroup name="attlist.jci">
		<xsd:attribute name="versie" type="xsd:string"/>
		<xsd:attribute name="verwijzing" type="xsd:string"/>
		<xsd:attribute name="onderdeel" type="xsd:string"/>
		<xsd:attribute name="korte-verwijzing" type="xsd:string"/>
		<xsd:attribute name="unieke-verwijzing" type="xsd:string"/>
	</xsd:attributeGroup>

	<xsd:element name="toestand">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="bwb-inputbestand">
					<xsd:annotation>
						<xsd:documentation>De bestandsnaam die als input heeft gediend voor deze
                            toestand (m.a.w. waarop de wijzigingen zijn
                            toegepast)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="bwb-wijzigingen"/>
				<xsd:element ref="redactionele-correcties" minOccurs="0"/>
				<xsd:element ref="wetgeving">
					<xsd:annotation>
						<xsd:documentation>Dit is het rootelement van hetgeen de Sdu levert / uit de
                            EGW client komt.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="inwerkingtreding" type="xsd:date" use="optional">
				<xsd:annotation>
					<xsd:documentation>Dit is de datum inwerkingtreding die heeft geleid tot deze
                        toestand.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="bwb-id"/>
			<xsd:attribute name="bwb-ng-vast-deel"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="bwb-wijzigingen">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="unbounded" minOccurs="0" ref="bwb-wijziging"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="bwb-wijziging">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="label"/>
				<xsd:element minOccurs="0" ref="terugwerkend.datum"/>
				<xsd:element minOccurs="0" ref="publicatie"/>
			</xsd:sequence>
			<xsd:attribute name="idref" type="xsd:NCName"/>
			<xsd:attribute default="officieel" name="oorzaak">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="officieel"/>
						<xsd:enumeration value="redactioneel"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="redactionele-correcties">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="unbounded" minOccurs="0" ref="redactionele-correctie"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="redactionele-correctie">
		<xsd:complexType>
			<xsd:attribute name="label-id" use="required"/>
			<xsd:attribute name="oud-revisie-id" use="required"/>
			<xsd:attribute name="nieuw-revisie-id" use="required"/>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
